@model Cat

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body>
    <h1>Update Cat</h1>
    <div>
        <form asp-action="Update" method="post" class="form-control">
            <div>
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" />
            </div>
            <div>
                <label asp-for="Age"></label>
                <input asp-for="Age" class="form-control" />
            </div>
            <div>
                <label asp-for="Breed"></label>
                <input asp-for="Breed" class="form-control" />
            </div>
            <div>
                <label asp-for="Img_Url"></label>
                <input asp-for="Img_Url" class="form-control" />
            </div>
            <input type="submit" value="Update" class="btn btn-primary" asp-action="Update" />
        </form>
    </div>
</body>
</html>